{
  "name": "CondominioApp",
  "private": true,
  "scripts": {
    "build": "turbo run build",
    "dev": "turbo run dev",
    "lint": "turbo run lint",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "check-types": "turbo run check-types",
    "validate:versions": "node scripts/validate-versions.js",
    "preinstall": "node scripts/validate-versions.js || true"
  },
  "devDependencies": {
    "@eslint/compat": "^2.0.1",
    "prettier": "^3.7.4",
    "turbo": "^2.7.2",
    "typescript": "5.9.2"
  },
  "engines": {
    "node": ">=18"
  },
  "packageManager": "bun@1.3.6",
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "dependencies": {
    "@types/react": "18.3.3",
    "@types/react-dom": "18.3.0",
    "lucide-react": "^0.562.0",
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "react-hook-form": "^7.71.0"
  }
}
